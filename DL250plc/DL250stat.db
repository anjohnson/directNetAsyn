record(mbbi, "$(name):mode")
{
	field(DESC,"CPU mode")
	field(DTYP,"DirectNet PLC via ASYN")
	field(INP, "@$(plc) SP11")
	field(NOBT,"7")
	field(SCAN,"I/O Intr")
	field(ZRST,"RUN")
	field(ZRVL,"0x01")
	field(ZRSV,"NO_ALARM")
	field(ONST,"TERM/RUN")
	field(ONVL,"0x02")
	field(ONSV,"NO_ALARM")
	field(TWST,"TEST/RUN")
	field(TWVL,"0x04")
	field(TWSV,"NO_ALARM")
	field(THST,"TERM/TEST")
	field(THVL,"0x10")
	field(THSV,"MAJOR")
	field(FRST,"TERM/PROGRAM")
	field(FRVL,"0x20")
	field(FRSV,"MAJOR")
	field(FVST,"STOP")
	field(FVVL,"0x40")
	field(FVSV,"MAJOR")
}

