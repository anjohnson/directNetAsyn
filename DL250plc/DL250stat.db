# CPU Status Relays
record(bi, "$(name):SP11")
{
	field(DESC,"Forced run mode")
	field(DTYP,"DirectNet PLC via ASYN")
	field(INP, "@$(plc) SP11")
	field(ONAM,"RUN")
	field(ZNAM," ")
	field(SCAN,"5 second")
}
